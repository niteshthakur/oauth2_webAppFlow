<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>OAUTHServlet</servlet-name>
  	<display-name>OAUTHServlet</display-name>
  	<description></description>
  	<servlet-class>com.nit.gooapi.servlets.OAUTHServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OAUTHPermissionServlet</servlet-name>
  	<display-name>OAUTHPermissionServlet</display-name>
  	<description></description>
  	<servlet-class>com.nit.gooapi.servlets.OAUTHPermissionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<display-name>UserServlet</display-name>
  	<description></description>
  	<servlet-class>com.nit.gooapi.servlets.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>OAUTHServlet</servlet-name>
  	<url-pattern>/OAUTHcallback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OAUTHPermissionServlet</servlet-name>
  	<url-pattern>/OAUTHPermissionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/UserInfo</url-pattern>
  </servlet-mapping>
</web-app>
